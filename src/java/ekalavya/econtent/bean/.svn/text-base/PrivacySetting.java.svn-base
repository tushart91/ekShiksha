
package ekalavya.econtent.bean;

import java.io.Serializable;

/**
 *
 * @author rkjangir
 */
public class PrivacySetting implements Serializable {
    // class variable declaration starts here
    private String userId;
    private boolean showEmail;
    private boolean showMobile;
    private boolean showQualification;
    private boolean showProfile;
    private boolean showLanguage;
    private boolean showTopic;
    // class variable declaration ends here

    public void setUserId( String userId) {
        this.userId = userId;
    }

    public String getUserId() {
        return userId;
    }

    public void setShowEmail(boolean showEmail) {
        this.showEmail = showEmail;
    }

    public boolean canShowEmail() {
        return showEmail;
    }

    public void setShowMobile(boolean showMobile) {
        this.showMobile = showMobile;
    }

    public boolean canShowMobile() {
        return showMobile;
    }

    public void setShowQualification(boolean showQualification) {
        this.showQualification = showQualification;
    }

    public boolean canShowQualification() {
        return showQualification;
    }

    public void setShowProfile(boolean showProfile) {
        this.showProfile = showProfile;
    }

    public boolean canShowProfile() {
        return showProfile;
    }

    public void setShowLanguage(boolean showLanguage) {
        this.showLanguage = showLanguage;
    }

    public boolean canShowLanguage() {
        return showLanguage;
    }

    public void setShowTopic(boolean showTopic) {
        this.showTopic = showTopic;
    }

    public boolean canShowTopic() {
        return showTopic;
    }
}
