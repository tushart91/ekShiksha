package ekalavya.econtent.bean;

/**
 *
 * @author rkjangir
 */
import java.io.Serializable;

public class URLBean implements Serializable{

        private int linkId;
        private int topicId;
        private String url;
        private String desc;
        private String author;
        private String level;
        private boolean isTranslatorAssigned;
        private boolean isReviewerAssigned;
        private boolean isPublisherAssigned;
        private String translatorId;
        private String reviewerId;
        private String publisherId;
        private int langId;
        private boolean verified;
        //setter methods
        public void setLinkId(int linkId) {
            this.linkId = linkId;
        }
        public void setTopicId(int topicId) {
            this.topicId = topicId;
        }
        public void setLangId(int langId) {
            this.langId = langId;
        }
        public void setUrl(String url) {
            this.url = url;
        }
        public void setDesc(String desc) {
            this.desc = desc;
        }
        public void setAuthor(String author) {
            this.author = author;
        }
        public void setLevel(String level) {
            this.level = level;
        }
        public void setIsTranslatorAssigned(boolean isTranslatorAssigned) {
            this.isTranslatorAssigned = isTranslatorAssigned;
        }
        public void setIsReviewerAssigned(boolean isReviewerAssigned) {
            this.isReviewerAssigned = isReviewerAssigned;
        }
        public void setIsPublisherAssigned(boolean isPublisherAssigned) {
            this.isPublisherAssigned = isPublisherAssigned;
        }
        public void setTranslatorId(String translatorId) {
            this.translatorId = translatorId;
        }
        public void setReviewerId(String reviewerId) {
            this.reviewerId = reviewerId;
        }
        public void setPublisherId(String publisherId) {
            this.publisherId = publisherId;
        }
        public void setVerified(boolean verified) {
            this.verified = verified;
        }
        //getter methods
        public int getLinkId() {
            return linkId;
        }
        public int getTopicId() {
            return topicId;
        }
        public int getLangId() {
            return langId;
        }
        public String getUrl() {
            return url;
        }
        public String getDesc() {
            return desc;
        }
        public String getAuthor() {
            return author;
        }
        public String getLevel() {
            return level;
        }
        public boolean getIsTranslatorAssigned() {
            return isTranslatorAssigned;
        }
        public boolean getIsReviewerAssigned() {
            return isReviewerAssigned;
        }
        public boolean getIsPublisherAssigned() {
            return isPublisherAssigned;
        }
        public String getTranslatorId() {
            return translatorId;
        }
        public String getReviewerId() {
            return reviewerId;
        }
        public String getPublisherId() {
            return publisherId;
        }
        public boolean isVerified() {
            return verified;
        }
}
